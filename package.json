{
  "name": "@hydre/redisearch-graphql",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "node --harmony-top-level-await --harmony-private-methods test/index.js",
    "coverage": "c8 --check-coverage node --harmony-top-level-await --harmony-private-methods test/index.js",
    "lint": "prettier-eslint $PWD/\"**/*.js\" --print-width 60 --list-different && eslint --color .",
    "format": "prettier-eslint $PWD/\"**/*.js\" --print-width 60 --write",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydreio/redisearch-graphql.git"
  },
  "author": "Hydre",
  "license": "UNLICENSE",
  "bugs": {
    "url": "https://github.com/hydreio/redisearch-graphql/issues"
  },
  "keywords": [
    "redisearch",
    "graphql",
    "hydre"
  ],
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "parserOpts": {
      "allowAwaitOutsideFunction": true
    },
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-private-methods"
    ]
  },
  "homepage": "https://github.com/hydreio/redisearch-graphql#readme",
  "dependencies": {
    "debug": "^4.1.1",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-numeric-separator": "^7.10.1",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@hydre/doubt": "^6.1.0",
    "@sidy/eslint-config-sidy": "^1.4.12",
    "babel-eslint": "^11.0.0-beta.2",
    "c8": "^7.2.0",
    "eslint": "^7.2.0",
    "eslint-plugin-unicorn": "^20.1.0",
    "prettier-eslint-cli": "^5.0.0",
    "tap-spec-emoji": "^6.1.2"
  }
}
